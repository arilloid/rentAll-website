   <div class="row rent-title">
        <div class="col">
            <h3 class="pt-5 pb-3">All property by City and Province</h3>
        </div>
    </div>
    <div class="row rentals">
        <div class="col-12 pt-4 pb-5">
            <div class="row">
                <div class="col-12">
                    {{#each rentalsGrouped}}
                        <div class="row p-2">
                            <h4>{{cityProvince}}</h4>
                        </div>   
                        <div class="row">
                            {{#each rentals}}
                                <div class="col-12 col-md-6 p-3">
                                    {{>rental}}
                                </div>
                            {{/each}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>


